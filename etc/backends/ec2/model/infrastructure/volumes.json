{
  "mixins":[
    {
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#storage"
      ],
      "term":"aws_ec2_ebs_volume",
      "scheme":"http://schemas.ec2.aws.amazon.com/occi/infrastructure/storage#",
      "title":"AWS EC2 extension - EBS volume",
      "location":"/mixin/aws_ec2_ebs_volume/",
      "attributes":{
        "com":{
          "amazon":{
            "aws":{
              "ec2":{
                "availability_zone":{
                  "Mutable":false,
                  "Description":"AWS availability zone identifier."
                },
                "state":{
                  "Mutable":false,
                  "Description":"AWS volume state."
                },
                "volume_type":{
                  "Mutable":false,
                  "Description":"AWS volume type."
                },
                "iops":{
                  "Mutable":false,
                  "Type":"number",
                  "Description":"AWS number of IOPS (per second)."
                },
                "encrypted":{
                  "Mutable":false,
                  "Type":"boolean",
                  "Description":"AWS encrypted volume yes/no."
                }
              }
            }
          }
        }
      }
    }
  ]
}
