{
  "mixins":[
    {
      "term":"r3_large",
      "scheme":"http://schemas.ec2.aws.amazon.com/occi/infrastructure/resource_tpl#",
      "title":"r3.large - 2 cores and 15.25 GB RAM",
      "location":"/mixin/resource_tpl/r3_large/",
      "depends":[
        "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
      ],
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#compute"
      ],
      "attributes":{
        "occi":{
          "compute":{
            "architecture":{
              "Mutable":false,
              "Type":"string",
              "Default":"x64"
            },
            "cores":{
              "Mutable":false,
              "Type":"number",
              "Default":2
            },
            "speed":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "memory":{
              "Mutable":false,
              "Type":"number",
              "Default":15.25
            }
          }
        }
      }
    }
  ]
}
